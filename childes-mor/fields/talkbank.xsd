<xs:complexType name="baseTerminatorType" mixed="false">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> Terminator for an utterance. </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Terminators">CHAT manual section on
            this topic...</a>
        </xs:documentation>
    </xs:annotation>

    <xs:attribute name="type" use="required">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="p">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation> Period. </xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#Period_Terminator">CHAT manual
                            section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="q">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation> Question mark. </xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#QuestionMark_Terminator">CHAT
                            manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="e">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation> Exclamation point. </xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#ExclamationMark_Terminator"
                               >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <!-- sterminator -->
                <xs:enumeration value="broken for coding">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+.</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#TranscriptionBreak_Terminator"
                               >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="trail off">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+...</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#TrailingOff_Terminator">CHAT
                            manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="trail off question">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+..?</xs:documentation>
                        <xs:documentation>
                            <a
                                    href="https://talkbank.org/manuals/CHAT.html#TrailingOffQuestion_Terminator"
                                    >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="question exclamation">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+!?</xs:documentation>
                        <xs:documentation>
                            <a
                                    href="https://talkbank.org/manuals/CHAT.html#QuestionExclamation_Terminator"
                                    >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="interruption">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+/.</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#Interruption_Terminator">CHAT
                            manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="interruption question">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+/?</xs:documentation>
                        <xs:documentation>
                            <a
                                    href="https://talkbank.org/manuals/CHAT.html#QuestionInterruption_Terminator"
                                    >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="self interruption">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+//.</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#SelfInterruption_Terminator"
                               >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="self interruption question">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+//?</xs:documentation>
                        <xs:documentation>
                            <a
                                    href="https://talkbank.org/manuals/CHAT.html#SelfInterruptedQuestion_Terminator"
                                    >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="quotation next line">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+&quot;/.</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#QuotationFollows_Terminator"
                               >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="quotation precedes">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>+&quot;.</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#QuotationPrecedes_Terminator"
                               >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="missing CA terminator">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>For heritage only</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#MissingCA_Terminator">CHAT
                            manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="technical break TCU continuation">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>&#x224B;</xs:documentation>
                        <xs:documentation>
                            <a href="https://talkbank.org/manuals/CHAT.html#TechnicalBreakTCUContinuation_Terminator"
                               >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>

                <xs:enumeration value="no break TCU continuation">
                    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                        <xs:documentation>&#x2248;</xs:documentation>
                        <xs:documentation>
                            <a
                                    href="https://talkbank.org/manuals/CHAT.html#NoBreakTCUContinuation_Terminator"
                                    >CHAT manual section on this topic...</a>
                        </xs:documentation>
                    </xs:annotation>
                </xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:attribute>
</xs:complexType>

<xs:element name="t">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> Main line terminator with optional %mor information. </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Terminators">CHAT manual section on
            this topic...</a>
        </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/MOR.html#Terminator_Alignment">MOR manual
            section on this topic...</a>
        </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="false">
        <xs:complexContent>
            <xs:extension base="baseTerminatorType">
                <xs:sequence>
                    <xs:element ref="mor" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:element>

<xs:element name="mt" type="baseTerminatorType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>Terminator on %mor line, important for %gra.</xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/MOR.html#Mor_Terminators">MOR manual section
            on this topic...</a>
        </xs:documentation>
    </xs:annotation>
</xs:element>

<xs:element name="pg">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> Group purely for phonetic annotation purposes. Note that there are no main
        line annotations allowed for the group. </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Phonological_Group">CHAT manual
            section on this topic...</a>
        </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="false">
        <xs:sequence>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="w"/>
                <xs:element ref="g"/>
                <xs:element ref="quotation"/>
                <xs:element ref="pause"/>

                <xs:element ref="internal-media"/>
                <xs:element ref="freecode"/>
                <xs:element ref="e"/>
                <xs:element ref="s"/>
                <xs:element ref="tagMarker"/>
                <xs:element ref="long-feature"/>
                <xs:element ref="nonvocal"/>
                <xs:element ref="overlap-point"/>
                <xs:element ref="underline"/>
                <xs:element ref="italic"/>
            </xs:choice>

            <!--
                Can optionally have one of actual or model.
                Order does not matter.
            -->
            <xs:choice minOccurs="0">
                <xs:sequence>
                    <xs:element ref="actual"/>
                    <xs:element ref="model" minOccurs="0"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element ref="model"/>
                    <xs:element ref="actual" minOccurs="0"/>
                </xs:sequence>
            </xs:choice>

            <xs:element ref="align" minOccurs="0" maxOccurs="1"/>

        </xs:sequence>
    </xs:complexType>


    <xs:unique name="model_ph_key">
        <xs:selector xpath="tb:model/tb:pw/tb:ph"/>
        <xs:field xpath="@id"/>
    </xs:unique>
    <xs:keyref refer="model_ph_key" name="model_ph_ref">
        <xs:selector xpath="tb:align/tb:col"/>
        <xs:field xpath="tb:modelref"/>
    </xs:keyref>


    <xs:unique name="actual_ph_key">
        <xs:selector xpath="tb:actual/tb:pw/tb:ph"/>
        <xs:field xpath="@id"/>
    </xs:unique>
    <xs:keyref refer="actual_ph_key" name="actual_ph_ref">
        <xs:selector xpath="tb:align/tb:col"/>
        <xs:field xpath="tb:actualref"/>
    </xs:keyref>
</xs:element>

<xs:element name="sg">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>Group purely for sign annotation purposes. Note that there are no main line annotations allowed for the group. Grouping brackets &#x3014; and &#x3015; may be used.</xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Sign_Group">CHAT manual
            section on this topic...</a>
        </xs:documentation>
    </xs:annotation>

    <xs:complexType mixed="false">
        <xs:sequence>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="w"/>
                <xs:element ref="g"/>
                <xs:element ref="quotation"/>
                <xs:element ref="pause"/>

                <xs:element ref="internal-media"/>
                <xs:element ref="freecode"/>
                <xs:element ref="e"/>
                <xs:element ref="s"/>
                <xs:element ref="tagMarker"/>
                <xs:element ref="long-feature"/>
                <xs:element ref="nonvocal"/>
                <xs:element ref="overlap-point"/>
                <xs:element ref="underline"/>
                <xs:element ref="italic"/>
            </xs:choice>

            <xs:element ref="sw" minOccurs="1" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="sw">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>Atomic unit on the %sin tier.</xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Sign_Tier">CHAT manual section on this topic...</a>
            <a href="https://talkbank.org/manuals/CHAT.html#Sign_Word">CHAT manual section on this topic...</a>
        </xs:documentation>
    </xs:annotation>

    <xs:complexType mixed="false">
        <xs:simpleContent>
            <xs:extension base="nonemptyStringType"/>
        </xs:simpleContent>
    </xs:complexType>
</xs:element>

<xs:element name="ss">
    <xs:annotation>
        <xs:documentation>
            Syllable stress.  Either primary or secondary.
        </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="false">
        <xs:attribute name="type" type="syllableStressType" use="required"/>
    </xs:complexType>
</xs:element>

<xs:simpleType name="syllableStressType">
    <xs:restriction base="xs:int">
        <xs:enumeration value="1">
            <xs:annotation>
                <xs:documentation> Primary stress, unicode 0x02c8 </xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="2">
            <xs:annotation>
                <xs:documentation> Secondary stress, unicode 0x02cc </xs:documentation>
            </xs:annotation>
        </xs:enumeration>
    </xs:restriction>
</xs:simpleType>

<xs:complexType name="actualPhoType" mixed="false">
    <xs:complexContent>
        <xs:extension base="phoType"/>
    </xs:complexContent>
</xs:complexType>

<xs:complexType name="modelPhoType" mixed="false">
    <xs:complexContent>
        <xs:extension base="phoType"/>
    </xs:complexContent>
</xs:complexType>

<xs:element name="actual" type="actualPhoType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Phonology_Tier">CHAT manual section on
            this topic...</a>
        </xs:documentation>
    </xs:annotation>

    <xs:unique name="actual_syll_start_key">
        <xs:selector xpath="tb:pw/tb:syll_start"/>
        <xs:field xpath="@label"/>
    </xs:unique>
    <xs:keyref name="actual_syll_start_ref" refer="actual_syll_start_key">
        <xs:selector xpath="tb:pw/tb:syll_end"/>
        <xs:field xpath="@label"/>
    </xs:keyref>

    <xs:unique name="actual_syll_end_key">
        <xs:selector xpath="tb:pw/tb:syll_end"/>
        <xs:field xpath="@label"/>
    </xs:unique>
    <xs:keyref name="actual_syll_end_ref" refer="actual_syll_end_key">
        <xs:selector xpath="tb:pw/tb:syll_start"/>
        <xs:field xpath="@label"/>
    </xs:keyref>
</xs:element>

<xs:element name="model" type="modelPhoType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Model_Tier">CHAT manual section on
            this topic...</a>
        </xs:documentation>
    </xs:annotation>

    <!-- GJH 2010-12-20
         syll_start and syll_end elements must have matching labels.
         Does not gaurentee that order of markers is correct.
    -->
    <xs:unique name="model_syll_start_key">
        <xs:selector xpath="tb:pw/tb:syll_start"/>
        <xs:field xpath="@label"/>
    </xs:unique>
    <xs:keyref name="model_syll_start_ref" refer="model_syll_start_key">
        <xs:selector xpath="tb:pw/tb:syll_end"/>
        <xs:field xpath="@label"/>
    </xs:keyref>

    <xs:unique name="model_syll_end_key">
        <xs:selector xpath="tb:pw/tb:syll_end"/>
        <xs:field xpath="@label"/>
    </xs:unique>
    <xs:keyref name="model_syll_end_ref" refer="model_syll_end_key">
        <xs:selector xpath="tb:pw/tb:syll_start"/>
        <xs:field xpath="@label"/>
    </xs:keyref>
</xs:element>

<!-- GJH 2010-12-13
     * Changed the content of phoType to be a list of ph elements.
     String content of the ph element is part of the ipa transcription.
     * Removed attribute 'lang'
-->
<xs:complexType name="phoType" mixed="false">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> Phonetic transcriptions of orthographic forms. </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Phonology_Tier">CHAT manual section on
            this topic...</a>
        </xs:documentation>
    </xs:annotation>

    <xs:sequence>
        <xs:element ref="pw" maxOccurs="unbounded" minOccurs="1"/>
    </xs:sequence>
</xs:complexType>

<xs:element name="pw">
    <xs:annotation>
        <xs:documentation> Phonetic transcription for a word. </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#Phonology_Word">CHAT manual section on
            this topic...</a>
        </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="false">
        <xs:choice maxOccurs="unbounded">

            <xs:element ref="ss"/>
            <xs:element ref="wk"/>
            <xs:element ref="ph">
                <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                    <xs:documentation> A phone in the IPA transcription. A phone may consist of one or more
                    unicode characters. </xs:documentation>
                </xs:annotation>
            </xs:element>

        </xs:choice>
    </xs:complexType>
</xs:element>

<!-- GJH 2010-12-13
     * Added element ph
-->
<xs:element name="ph">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>Specifies a syllable constituent. The type is one of constituentTypeType.
        Each constituent can constist of one or more phones identified by zero-based index of the
        parent phonetic rep.</xs:documentation>
    </xs:annotation>


    <!-- GJH 2010-12-13
         * Removed attributes offset and length.
         * Allowing string content in type
         * Added attribute 'id'
    -->
    <xs:complexType mixed="true">
        <xs:attribute name="sctype" type="constituentTypeType" use="optional" default="UK">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation> The syllable constituent type for this phone. </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <!--
            Each phone needs an id for reference in alignment.
            id is not an xs:ID to avoid having globally unique ids.  The constraint for this
            is found in the model and actual element declarations.
        -->
        <xs:attribute name="id" type="phKeyType" use="required">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation> Each phone is required to have a locally unique id. i.e., sibling ph
                elements cannot have the same id. </xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="hiatus" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation>
                    Used when two ph elements with sctype of 'N' are adjacent.
                    If hiatus is true, each nucleus is the root of its own syllable.
                    If hiatus is false, the pair of nuclei are considered a diphthong.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
</xs:element>

<xs:simpleType name="constituentTypeType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation>Valid syllable constituent labels.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
        <xs:enumeration value="SB">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Syllable boundary marker ('.')</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="SS">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Syllable stress (i.e., primary or secondary)</xs:documentation>
                <xs:documentation>Deprecated: use stress attribute of syll_start
                instead</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="LA">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Left appendix</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="O">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Onset</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="N">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Nucleus</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="C">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Coda</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="RA">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Right appendix</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="OEHS">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Onset of an empty headed syllable</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="AS">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Ambisyllabic</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
        <xs:enumeration value="UK">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
                <xs:documentation>Unknown</xs:documentation>
            </xs:annotation>
        </xs:enumeration>
    </xs:restriction>
</xs:simpleType>

<!-- GJH 2010-12-13
     * Added element align
-->
<xs:element name="align">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> This type represents the alignment of two phonetic representations.
        </xs:documentation>
    </xs:annotation>

    <!-- GJH 2010-12-13
         * Removed elements top and bottom
         * Removed attribute length
         * Added new sequence with a single, unbounded element named 'col'
         Each 'col' element represents a single column in the alignment.
         The attributes model and actual of the col element refer to ph ids
         in the model and actual elements respectively.  The constraint to
         ensure referenced elements exist is found in the declaration
         of the align element.
    -->
    <xs:complexType>
        <xs:sequence>
            <xs:element ref="col" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
</xs:element>

<xs:element name="col">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> A single column in the alignment.
        </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="false">
        <!-- At least one of modelref or actualref must exist,
             order does not matter -->
        <xs:choice>
            <xs:sequence>
                <xs:element ref="modelref"/>
                <xs:element ref="actualref" minOccurs="0"/>
            </xs:sequence>
            <xs:sequence>
                <xs:element ref="actualref"/>
                <xs:element ref="modelref" minOccurs="0"/>
            </xs:sequence>
        </xs:choice>
    </xs:complexType>

</xs:element>

<xs:element name="modelref" type="phKeyType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> model reference
        </xs:documentation>
    </xs:annotation>
</xs:element>
<xs:element name="actualref" type="phKeyType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> actual reference
        </xs:documentation>
    </xs:annotation>
</xs:element>

<xs:simpleType name="phKeyType">
    <xs:restriction base="nonemptyStringType">
        <xs:pattern value="ph[0-9]+"/>
    </xs:restriction>
</xs:simpleType>

<!-- End of MUN defined types -->


<!-- GJH 2010-12-20
     * Removed simpleType 'utteranceTypeType'
     <xs:simpleType name="utteranceTypeType">
     <xs:restriction base="xs:NMTOKEN">
     <xs:enumeration value="phon"/>
     <xs:enumeration value="morpho"/>
     </xs:restriction>
     </xs:simpleType>
-->

<xs:simpleType name="annotationTypeType">
    <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
        <xs:documentation> Utterance annotation type. </xs:documentation>
        <xs:documentation>
            <a href="https://talkbank.org/manuals/CHAT.html#DependentTiers">CHAT manual section on
            this topic...</a>
        </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
        <xs:enumeration value="addressee">
            <xs:annotation xmlns="http://www.w3.org/1999/xhtml">
